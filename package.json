{
  "name": "ember-bug-widget",
  "version": "2.1.2",
  "description": "Bug reporting widget for Ember applications",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "scripts": {
    "build": "ember build",
    "deploy": "ember github-pages:commit --message \"Deploy gh-pages from commit $(git rev-parse HEAD)\"; git push; git checkout -",
    "start": "ember server",
    "test": "ember try:each"
  },
  "repository": "https://github.com/elwayman02/ember-bug-widget",
  "engines": {
    "node": ">= 0.10.0"
  },
  "author": "Jordan Hawker <hawker.jordan@gmail.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/elwayman02/ember-bug-widget/issues"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.2",
    "broccoli-merge-trees": "^1.1.1",
    "broccoli-static-compiler": "^0.2.2",
    "ember-cli": "2.8.0",
    "ember-cli-app-version": "^2.0.0",
    "ember-cli-content-security-policy": "0.5.0",
    "ember-cli-dependency-checker": "^1.2.0",
    "ember-cli-github-pages": "0.1.2",
    "ember-cli-htmlbars-inline-precompile": "^0.3.1",
    "ember-cli-inject-live-reload": "^1.4.0",
    "ember-cli-jshint": "^2.0.1",
    "ember-cli-qunit": "^3.0.2",
    "ember-cli-release": "1.0.0-beta.1",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-test-loader": "^1.1.0",
    "ember-cli-uglify": "^1.2.0",
    "ember-disable-prototype-extensions": "^1.1.0",
    "ember-export-application-global": "^1.0.5",
    "ember-load-initializers": "^0.5.1",
    "ember-resolver": "^2.0.3",
    "ember-try": "^0.2.2",
    "ember-watson": "^0.8.0",
    "loader.js": "^4.0.5"
  },
  "keywords": [
    "ember-addon",
    "bug",
    "widget"
  ],
  "dependencies": {
    "ember-cli-babel": "^5.1.6",
    "ember-cli-htmlbars": "^1.0.3",
    "ember-cli-sass": "5.5.1"
  },
  "ember-addon": {
    "configPath": "tests/dummy/config",
    "demoURL": "http://jhawk.co/ember-bug-widget-demo"
  }
}
